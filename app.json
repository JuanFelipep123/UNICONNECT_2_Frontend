{
  "expo": {
    "name": "uniconnect_2",
    "slug": "uniconnect_2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "uniconnect2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juanfe004.uniconnect2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.juanfe004.uniconnect2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "17aa4738-1808-4eaa-b726-bde0a9318b77"
      }
    },
    "owner": "juanfe_004",
    "updates": {
      "url": "https://u.expo.dev/17aa4738-1808-4eaa-b726-bde0a9318b77",
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
